{
    "name": "Distribution-Schema",
    "author": "denis weerasiri",
    "version": "1.0.0",
    "description": "represents the distribution of a Docker Image in a Registry",
    "associated-tool": "Docker-CLI",
    "participating-entities": [
        {
            "name": "Image-Schema",
            "version": "1.0.0",
            "role": "distributed by",
            "cardinality": {
                "min-value": "1",
                "max-value": "1"
            }
        },
        {
            "name": "Registry-Schema",
            "version": "1.0.0",
            "role": "distribute",
            "cardinality": {
                "min-value": "0",
                "max-value": "INF"
            }
        }
    ]
}
